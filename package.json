{
  "name": "react-component-boilerplate",
  "version": "1.0.0",
  "description": "a simple boilerplate for react components",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test/.setup.js test/**/*-test.js",
    "lint": "./node_modules/.bin/eslint src/**/*",
    "build": "npm run lint && ./node_modules/.bin/babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdir/react-component-boilerplate.git"
  },
  "keywords": [
    "react",
    "component",
    "boilerplate"
  ],
  "author": "Jason Sommer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdir/react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/jsdir/react-component-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.4.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "jsdom": "^8.0.1",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.3.1"
  },
  "peerDependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
